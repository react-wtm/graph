{
  "presets": ["module:metro-react-native-babel-preset"],
  "sourceMaps": true,
  "plugins": [
    "transform-remove-strict-mode",
    "@babel/plugin-transform-react-jsx-source",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "import",
      {
        "libraryName": "@ant-design/react-native"
      }
    ]
  ]
}
